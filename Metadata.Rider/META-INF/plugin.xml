<idea-plugin require-restart="true">
  <change-notes>Initial version</change-notes>
  <id>com.jetbrains.rider.plugins.powersharp</id>
  <name>PowerSharp</name>
  <version>2024.2.0</version>
  <vendor url="https://github.com/DmitryStrakhov/PowerSharp">Dmitry Strakhov</vendor>
  <idea-version since-build="242.21829" until-build="242.*" />
  <depends>com.intellij.modules.rider</depends>
  <description><![CDATA[<p>PowerSharp extends Rider by a few nice features like 'Create Instance', 'Create Tests', 'Edit Method' and more. Please follow project's github repository for more details.</p>]]></description>
  
  <actions>
    <action id="PowerSharp.EditMethod" class="PowerSharp.EditMethodAction" />
    <action id="PowerSharp.EditPropertyGetter" class="PowerSharp.EditPropertyGetterAction" />
    <action id="PowerSharp.EditPropertySetter" class="PowerSharp.EditPropertySetterAction" />
    <action id="PowerSharp.NewMemberLine" class="PowerSharp.NewMemberLineAction" />
    <action id="PowerSharp.ForceMethodCall" class="PowerSharp.ForceMethodCallAction" />
  </actions>
</idea-plugin>
